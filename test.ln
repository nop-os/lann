# a fast, simple and safe programming language

# smart error return handling:

func foo: begin
  if get(args, 0) < 5: error(0)
  else: get(args, 0) + 1
end

func bar: begin
  let x: foo(3)
  
  if is_error(x): begin
    printf("aw snap, failed! error: []\n", err)
    back
  end
  
  printf("hi! x = []\n", x)
end

# lambda functions:

func bar begin
  let x begin
    let a: 3
    let b: 2
    
    a + b
  end
  
  printf("[]\n", x)
end

